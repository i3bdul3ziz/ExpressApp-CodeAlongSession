<h1>People Page</h1>

<table>
    <tr>
        <th> First Name </th>
        <th> Last Name </th>
    </tr>
<% data.forEach(function(data, index) { %>
    <tr>
        <td><%= data.firstName %></td>
        <td><%= data.lastName %></td>
    </tr>
    <% }) %>
</table>

<a href="createPerson"><button>New Person</button></a>
<a href="updatePerson"><button>Edit Person</button></a>
<a href="deletePerson"><button>Delete Person</button></a>
