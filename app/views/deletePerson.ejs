<h1> Delete Person </h1>

<table>
    <tr>
        <th> First Name </th>
        <th> Last Name </th>
    </tr>
    <% data.forEach(function(data,index){ %>
    <tr>
        <td><%= index %></td>
        <td><%= data.firstName %></td>
        <td><%= data.lastName %></td>
    </tr>
    <% }) %>
</table>
<form method="POST" action="/api/deletedPerson">
    <label for="">Number Of Index</label> <br>
    <input type="number" name="index" id="index"> 

    <br> 

    <input type="submit" name="Delete" value="Delete">
</form>